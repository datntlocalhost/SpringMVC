<mapper namespace="com.runsystem.datnt.database.mapper.IMapper">
	
	<select id="search"  parameterType="Student" resultType="FullStudentInfo">
		SELECT s.studenid, s.studentname, s.studentcode, si.infoid, si.address, si.averagescore, si.dateofbirth 
		FROM student s, studentinfo si
		WHERE  s.studentid = si.studentid 
