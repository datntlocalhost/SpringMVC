jQuery(document).ready(function() {

	$("#login").on('submit', function(event) {
		var usernameRegex = /[a-zA-Z0-9]{2,}/;
		

		event.preventDefault();
		if ($("#form-username").val() == "") {
			event.preventDefault();
			$("#invalid-usr").html("Invalid username");
			$("#form-username").focus();
		}
	});
});
