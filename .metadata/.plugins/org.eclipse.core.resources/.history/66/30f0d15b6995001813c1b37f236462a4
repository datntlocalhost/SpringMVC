package com.runsystem.datnt.business;

import com.runsystem.datnt.dto.Student;

public class InitSearch {
	
	public static void init(Student student) {
		if (student.getStudentName().length() == 0) {
			student.setStudentName(null);
		}
		if (student.getStudentCode().length() == 0) {
			student.setStudentCode(null);
		}
	}

}
