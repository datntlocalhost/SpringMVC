package com.runsystem.datnt.database.mapper;

import java.util.List;

public interface IMapper<T,T1> {
	
	public int insert(T1 param);
	
	public int update(T1 param);
	
	public int delete(T1 param);
	
	public T selectOne(T1 param);
	
	public List<T> selectAll();
	
	public List<T> search(T1 param);

}
