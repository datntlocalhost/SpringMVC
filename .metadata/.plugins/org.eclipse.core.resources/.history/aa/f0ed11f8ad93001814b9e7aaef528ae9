import org.springframework.context.annotation.Configuration;

@Configuration
@ComponentScan(basePackages = {"com.runsystem.datnt.database.service"})
public class AutoConfig {
	
	@Bean
	public SqlSessionFactoryBean sqlSessionFactory() {
		
	}

}
